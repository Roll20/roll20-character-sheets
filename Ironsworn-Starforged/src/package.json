{
  "name": "ironsworn-starforged",
  "version": "2.2.0",
  "description": "Roll20 character sheet for the RPG ironsworn-starforged",
  "main": "Ironsworn-Starforged.html",
  "scripts": {
    "build:pug": "NODE_OPTIONS=--max-old-space-size=8192 pug -w . -o ../dist -P",
    "build:pug:win": "pug -w . -o ../dist -P",
    "build:css": "stylus -w . -o ../dist",
    "lint:pug": "pug-lint",
    "lint:css": "stlint",
    "gulp:build": "gulp build",
    "gulp:watch": "gulp watch"
  },
  "author": "Ayethin",
  "dependencies": {
    "axios": "1.7.4",
