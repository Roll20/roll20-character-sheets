    "gulp": "5.0.0",
    "gulp-pug": "5.0.0",
    "gulp-stylus": "2.7.0",
    "pug": "3.0.2",
    "pug-cli": "1.0.0-alpha6",
    "pug-lint": "2.6.0",
    "stlint": "1.0.65",
    "stylus": "0.54.8"
  },
  "devDependencies": {
    "gulp-data": "1.3.1",
    "gulp-merge-json": "2.1.1",
    "lodash": "4.17.21"
  }
