{
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "globals": {
        "getAttrs": "readonly",
        "getSectionIDs": "readonly",
        "on": "readonly",
        "setAttrs": "readonly"
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "_"
            }
        ]
    }
}