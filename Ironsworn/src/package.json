{
  "name": "ironsworn",
  "version": "2.3.0",
  "description": "Roll20 character sheet for the RPG ironsworn",
  "main": "Ironsworn.html",
  "scripts": {
    "build:pug": "NODE_OPTIONS=--max-old-space-size=8192 pug -w . -o ../dist -P",
    "build:pug:win": "pug -w . -o ../dist -P",
    "build:css": "stylus -w . -o ../dist",
    "lint:pug": "pug-lint",
    "lint:css": "stlint",
    "gulp:build": "gulp build",
    "gulp:watch": "gulp watch"
  },
  "author": "Ayethin",
  "dependencies": {
    "gulp": "4.0.2",
    "gulp-data": "1.3.1",
    "gulp-pug": "4.0.1",
    "gulp-stylus": "2.7.0",
    "pug": "3.0.1",
    "pug-cli": "1.0.0-alpha6",
    "pug-lint": "2.6.0",
    "stlint": "1.0.62",
    "stylus": "0.54.7"
  }
}
