{
  "version": "1.0.0",
  "scripts": {
    "start": "npm-run-all --parallel sass pug",
    "sass": "sass ./src/torchbearer.scss ./torchbearer.css -w --no-source-map --no-charset",
    "pug": "pug ./src/torchbearer.pug -o . -w --pretty"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "pug": "^3.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "sass": "^1.77.3"
  }
}
