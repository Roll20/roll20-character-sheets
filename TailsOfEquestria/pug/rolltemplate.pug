rolltemplate.sheet-rolltemplate-toe
  table
    thead: tr: th {{charName}} &#x21D2; {{attr}}
    tbody
      tr: td Result {{result}}
      
      | {{#updowngrade}}
      tr: td.updowngrade
        p
          span Up/Downgrade:
          span {{updowngrade}}
        p {{updowngrade_note}}
      | {{/updowngrade}}
      
      | {{#notes}}
      tr: td.notes
        div Notes:
        div {{notes}}
      | {{/notes}}
